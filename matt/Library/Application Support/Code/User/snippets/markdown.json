{
	"Table": {
		"prefix": "ta",
		"body": [
			"| **$1** | **$2** |",
			"| -- | -- |",
			"| $3 | $4 |"
		]
	},
	"Table Row": {
		"prefix": "tr",
		"body": "| $1 | $2 |"
	},
	"Italicized": {
		"prefix": "it",
		"body": "_$1_"
	},
	"Bold": {
		"prefix": "bo",
		"body": "**$1**"
	},
	"Inline Code": {
		"prefix": "ic",
		"body": "`$1`"
	},
}
