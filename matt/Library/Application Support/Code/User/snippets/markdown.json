{
	"Markdown table": {
		"prefix": "ta",
		"body": [
			"| **$1** | **$2** |",
			"| -- | -- |",
			"| $3 | $4 |"
		],
		"description": "Markdown table"
	},
	"Markdown row": {
		"prefix": "tr",
		"body": [
			"| $1 | $2 |"
		],
		"description": "Markdown row"
	},
	"Bold text": {
		"prefix": "bo",
		"body": "**$1**",
		"description": "Bold text"
	},
	"Italicized text": {
		"prefix": "it",
		"body": "_$1_",
		"description": "Italicized text"
	}
}
